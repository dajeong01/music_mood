<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.music_backend.domain.spotify.genre.GenreMapper">

    <select id="getAllGenres" resultType="map">
        SELECT
        genre_id,
        genre_name
        FROM genre_tb
        ORDER BY genre_id ASC
    </select>


</mapper>